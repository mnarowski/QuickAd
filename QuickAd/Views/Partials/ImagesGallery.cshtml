@model IEnumerable<QuickAd.Models.Image>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vpath)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vextension)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisPrivate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VidAdvertise)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vpath)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vextension)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisPrivate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VidAdvertise)
        </td>
        <td>
        </td>
    </tr>
}

</table>
